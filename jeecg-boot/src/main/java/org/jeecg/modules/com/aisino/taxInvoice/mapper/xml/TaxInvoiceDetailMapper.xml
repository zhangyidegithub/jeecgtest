<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.com.aisino.taxInvoice.mapper.TaxInvoiceDetailMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  TAX_INVOICE_DETAIL 
		WHERE
			 dataid = #{id} 		
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.jeecg.modules.com.aisino.taxInvoice.entity.TaxInvoiceDetail">
		SELECT * 
		FROM  TAX_INVOICE_DETAIL
		WHERE
			 dataid = #{id} 		
	</select>
</mapper>
